{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-airbnb"
	],
	"jsRules": {},
	"rules": {
		"strict-boolean-expressions": [
			false,
			"allow-undefined-union",
			"allow-null-union",
			"allow-number",
			"allow-string",
			"allow-mix"
		],
		"ter-arrow-parens": true,
		"no-unused-variable": true,
		"indent": [
			true,
			"tabs",
			1
		],
		"ter-indent": [
			true,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"object-literal-sort-keys": false,
		"max-line-length": [
			true,
			200
		],
		"function-name": false,
		"align": [
			true
		],
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "always",
					"typeLiterals": "always",
					"imports": "ignore"
				},
				"singleline": "never"
			}
		]
	},
	"rulesDirectory": []
}
