# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = tab
indent_size = 4

[package*.json]
# used by npm that modifies it
indent_style = space
indent_size = 2

[typings.json]
# used by typings tool that modifies it
indent_style = space
indent_size = 2
